<h1>Your Feed</h1>


<% @items.each do |item| %>
  <% if item.class == TextItem %>
     text item: <p><%=item.body%></p>
  <% elsif item.class == ImageItem %>
    image item: <p><%= item.url %></p>  
  <% elsif item.class == LinkItem %>
    link item: <p><%= item.url %></p>
  <% end %>
<% end %>
<%= paginate @stream_items %>
