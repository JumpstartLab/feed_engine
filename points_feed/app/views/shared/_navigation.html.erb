<div class="navbar navbar-fixed-top">
  <div class="navbar_inner">
      <div class="container">
      <a class="btn btn-navbar" data-target=".nav-collapse" data-toggle="collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>
      <a class="brand" href="/"><img src='/assets/logo.png'></img></a>
      <div class="container nav-collapse">
        <ul class="nav">
          <!-- <li><%= link_to "Link1", "/path1"  %></li> -->
        </ul>
          <% if current_user %>
            <ul class='nav pull-right' id="menu">
              <form class="navbar-search pull-left" action="" style='margin-right: 20px; margin-top: 10px;'>
                <input type="text" class="search-query span2 typeahead" placeholder="Search for Users">
              </form>
              <li>
                <div style='margin-top: 10px; font-size: 18px;'>
                  <%= link_to "http://#{current_user.display_name}.pointsfeed.in" do %>
                    <img src="<%= current_user.avatar %>" style='height: 20px; margin-right: 5px; margin-top: 0px;'></img>
                    <%= current_user.display_name %>
                  <% end %>
                </div>
                <ul class="active">
                  <li><%= link_to 'Dashboard', dashboard_url(:subdomain => false) %></li>
                  <li><%= link_to 'Sign Out', destroy_user_session_url(:subdomain => false), :method => :delete %></li>
                </ul>
              </li>
          <% else %>
            <ul class='nav pull-right'>
              <li><%= link_to 'Sign Up', new_user_registration_url(:subdomain => false) %></li>
              <li><%= link_to 'Sign In', new_user_session_url(:subdomain => false) %></li>
          <% end %>
        </ul>
      </div><!--/.nav-collapse -->
    </div>
  </div>
</div>